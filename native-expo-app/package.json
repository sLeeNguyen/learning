{
  "name": "native-expo-app",
  "private": true,
  "version": "1.0.0",
  "main": "src/App.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "build:web": "expo build:web",
    "setup:husky": "husky install",
    "lint:dev-check": "eslint --config ./.eslintrc.js . --ext .jsx,.js,.tsx,.ts",
    "lint:check": "eslint --config ./.eslintrc-staged.js . --ext .jsx,.js,.tsx,.ts",
    "lint:fix": "yarn run lint:check --fix",
    "format": "prettier '*/**/*.{ts,js,jsx,tsx,json,yml,md}' --write",
    "prepare": "husky install",
    "devtools": "react-devtools"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.6",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "deepmerge": "^4.2.2",
    "expo": "~45.0.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "expo-updates": "~0.13.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-paper": "^4.12.2",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "0.17.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@react-native-community/eslint-config": "^3.0.2",
    "@types/react": "^18.0.14",
    "@types/react-native": "~0.66.13",
    "@types/react-native-vector-icons": "^6.4.10",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "~8.12.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "react-devtools": "^4.24.7",
    "typescript": "~4.3.5"
  },
  "resolutions": {
    "@types/react": "^18.0.14"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --config ./.eslintrc-staged.js --cache --fix",
    "*.{json,yml,md}": [
      "prettier --write"
    ]
  }
}
